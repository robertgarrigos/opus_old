{
  "name": "ziishaned/opus",
  "description": "Open source knowledge base application for Teams",
  "version": "1.3.4",
  "homepage": "https://github.com/ziishaned/opus",
  "license": "YUMMY License",
  "authors": [
    {
      "name": "Zeeshan Ahmad",
      "email": "ziishaned@gmail.com",
      "homepage": "https://github.com/ziishaned",
      "role": "developer"
    },
    {
      "name": "Michael Stokoe",
      "email": "michael@koecreative.com",
      "homepage": "https://koecreative.com",
      "role": "developer - ported to laravel 5.8"
    }
  ],
  "keywords": [
    "teams",
    "knowledgebase",
    "opus",
    "laravel",
    "wiki"
  ],
  "type": "project",
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.8.*",
    "cviebrock/eloquent-sluggable": "^4.1",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "intervention/image": "^2.3",
    "fenos/notifynder": "^4.0",
    "doctrine/dbal": "^2.5",
    "laravel/tinker": "^1.0",
    "graham-campbell/exceptions": "^11.2",
    "baum/baum": "dev-master",
    "filp/whoops": "^2.3",
    "sentry/sentry-laravel": "1.0.1"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "2.*",
    "barryvdh/laravel-debugbar": "^3.2"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.dist', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
          "name": "baum/baum",
          "version": "dev-master",
          "source": {
              "url": "https://github.com/stokoe0990/baum",
              "type": "git",
              "reference": "master"
          },
          "autoload": {
              "classmap": [""]
          }
      }
    }
  ]
}
